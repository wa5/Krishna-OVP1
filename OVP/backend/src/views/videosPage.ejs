  <body>
    
<nav class="navbar navbar-expand-md navbar-dark fixed-top bg-dark">
  <div class="container-fluid">
    <a class="navbar-brand" href="#">OVP</a>
    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarCollapse">
      <ul class="navbar-nav me-auto mb-2 mb-md-0">
        <li class="nav-item">
          <a class="nav-link active" aria-current="page" href="http://localhost:8000/api/home">Home</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="http://localhost:8000/api/videospage">All videos</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="#">logs</a>
        </li>
        <li class="nav-item">
          <a class="btn  btn-danger" href="http://localhost:8000/api/uploadvideos" role="button">upload videos &raquo;</a>
   
        </li>
        
      </ul>
     
    </div>
  </div>
</nav>

<main class="container">
  <div class="bg-light p-5 rounded">
    <h1>all movies prent in server</h1>
    <div class="row">
      <% users.forEach(function(user){ %>
      <div class="col-3 mx-auto">
        <div>
          <center>
            <video id="videoPlayer" controls width="100%" height="100%">
              <% var url_text='http://localhost:8000/videos/' %>
              <source src="<%= url_text+user.video_url%>" type="video/mp4" />
            </video>
          </center>

          <% var id_text='http://localhost:8000/singlepage/' %>

          <a
            href="<%= id_text+user._id%>"
            style="text-decoration: none; text-align: center"
            ><p><%= user.title%></p>

            <div class="d-grid gap-2 mx-auto">
              <button class="btn btn-warning" type="button">
                Update Fields
              </button>
            </div>
          </a>
        </div>
      </div>
      <% }); %>
    </div>
  </div>
</main>
